{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "crons": [
    {
      "path": "/api/cron/cleanup-messages",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/cleanup-nonces",
      "schedule": "0 */6 * * *"
    }
  ]
}